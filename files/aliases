# Docker
alias docker-minikube="eval $(minikube docker-env)"
alias docker-default="eval $(minikube docker-env -u)"

# Scripts
alias mvn-build-run="$UTILS/scripts/mvn_install.sh" # Java Maven
alias push="$UTILS/scripts/push.sh"

# Folders
alias windows-home="cd /mnt/c/Users/Lucas\ Perez"

# Utils
alias refresh="source $HOME/.zshrc"
alias utf-clear="find . -name '*.java' -exec sed -ri '${/^$/d;}' {} \;"
alias unix2all="find . -type f -print0 | xargs -0 dos2unix"

