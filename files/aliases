alias docker-minikube="eval $(minikube docker-env)"
alias docker-default="eval $(minikube docker-env -u)"

alias docker-stop="docker stop $(docker ps -aq)"
alias docker-rm="docker rm $(docker ps -aq)"
alias docker-clear="docker stop $(docker ps -aq); docker rm $(docker ps -aq)"

alias push="$UTILS/scripts/push.sh"

alias refresh="source $HOME/.zshrc"

